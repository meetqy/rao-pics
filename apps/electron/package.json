{
  "name": "@rao-pics/electron",
  "version": "1.0.0-alpha.13",
  "description": "远程访问 Eagle 素材资源",
  "homepage": "https://rao.pics",
  "repository": {
    "type": "git",
    "url": "https://github.com/meetqy/rao-pics.git"
  },
  "author": {
    "name": "meetqy",
    "email": "meetqy@qq.com"
  },
  "main": "./out/main/index.js",
  "scripts": {
    "build": "pnpm typecheck && pnpm with-env electron-vite build",
    "build:mac": "pnpm with-env node ./electron-builder/mac.cjs",
    "build:win": "pnpm with-env node ./electron-builder/win.cjs",
    "clean": "git clean -xdf node_modules out releases .turbo",
    "dev": "pnpm with-env electron-vite dev",
    "format": "prettier --check \"**/*.{mjs,ts,md,json}\"",
    "postinstall": "electron-builder install-app-deps",
    "lint": "eslint .",
    "releases": "git clean -xdf releases && pnpm build:mac && pnpm build:win",
    "start": "electron-vite preview",
    "typecheck": "pnpm typecheck:node && pnpm typecheck:web",
    "typecheck:node": "tsc --noEmit -p tsconfig.node.json --composite false",
    "typecheck:web": "tsc --noEmit -p tsconfig.web.json --composite false",
    "with-env": "dotenv -e ../../.env --"
  },
  "prettier": "@rao-pics/prettier-config",
  "dependencies": {
    "@electron-toolkit/preload": "^2.0.0",
    "@electron-toolkit/utils": "^2.0.0",
    "@heroicons/react": "^2.0.18",
    "@rao-pics/api": "workspace:^",
    "@rao-pics/constant": "workspace:^",
    "@rao-pics/db": "workspace:*",
    "@rao-pics/rlog": "workspace:^",
    "@rao-pics/trpc": "workspace:^",
    "@sentry/electron": "^4.14.0",
    "electron-updater": "^6.0.0",
    "ip": "^1.1.8",
    "lodash": "^4.17.21",
    "qrcode.react": "^3.1.0",
    "ws": "^8.14.2"
  },
  "devDependencies": {
    "@electron-toolkit/tsconfig": "^1.0.1",
    "@electron/notarize": "^1.2.3",
    "@rao-pics/eslint-config": "workspace:^",
    "@rao-pics/prettier-config": "workspace:^",
    "@rao-pics/tailwind-config": "workspace:*",
    "@rao-pics/tsconfig": "workspace:*",
    "@sentry/vite-plugin": "^2.7.1",
    "@types/ip": "^1.1.0",
    "@types/lodash": "^4.14.197",
    "@types/node": "^18.17.6",
    "@types/react": "^18.2.20",
    "@types/react-dom": "^18.2.7",
    "@vitejs/plugin-react": "^4.0.3",
    "electron": "28.2.3",
    "electron-builder": "24.9.1",
    "electron-devtools-installer": "^3.2.0",
    "electron-vite": "^1.0.25",
    "eslint": "^8.47.0",
    "prettier": "^3.0.2",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "tailwindcss": "3.3.6",
    "typescript": "^5.1.6",
    "vite": "^4.4.2"
  },
  "productName": "Rao Pics"
}
