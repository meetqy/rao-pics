{
  "name": "@rao-pics/db",
  "version": "0.1.0",
  "private": true,
  "exports": {
    ".": "./index.ts"
  },
  "main": "./index.ts",
  "scripts": {
    "build": "pnpm reset",
    "clean": "rm -rf .turbo node_modules",
    "dev": "prisma studio -b none",
    "format": "prettier --check \"**/*.{mjs,ts,md,json}\"",
    "postinstall": "pnpm push",
    "lint": "eslint .",
    "push": "prisma db push",
    "reset": "pnpm push --force-reset",
    "test": "cross-env NODE_ENV=development vitest --poolOptions.threads.singleThread --ui --coverage",
    "typecheck": "tsc --noEmit"
  },
  "prettier": "@rao-pics/prettier-config",
  "eslintConfig": {
    "extends": [
      "@rao-pics/eslint-config/base"
    ]
  },
  "dependencies": {
    "@prisma/client": "5.6.0",
    "@rao-pics/constant": "workspace:^",
    "@rao-pics/rlog": "workspace:^",
    "fs-extra": "^11.1.1"
  },
  "devDependencies": {
    "@rao-pics/eslint-config": "workspace:*",
    "@rao-pics/prettier-config": "workspace:*",
    "@rao-pics/tsconfig": "workspace:*",
    "@types/node": "^18.17.6",
    "cross-env": "^7.0.3",
    "eslint": "^8.47.0",
    "prisma": "^5.6.0",
    "typescript": "^5.1.6",
    "vitest": "^1.0.0"
  }
}
